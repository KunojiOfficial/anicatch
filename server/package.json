    "nodemon": "^3.1.0",
    "prisma": "^5.22.0",
    "shelljs": "^0.8.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-hooks": "^1.1.2",
    "tsx": "^4.19.3",
    "typescript": "^5.4.5"
  }
