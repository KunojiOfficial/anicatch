{
  "name": "animon",
  "type": "commonjs",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/index.js",
  "scripts": {
    "start": "npx tsc-watch --outDir ./dist --onSuccess \"ts-node ./scripts/run\"",
    "db": "npx prisma migrate dev --name dev && npx prisma studio",
    "build": "tsc && cpy --cwd=src '*.json' '../dist/' && cp -r src/assets dist/src/assets && cp -r src/locale dist/src/locale && cp -r src/web/views dist/src/web",
    "dev": "nodemon --ignore src/assets --exec tsx ./index.ts",
    "emojis": "tsx ./src/helpers/setupEmojis.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "prisma.schema": "./src/prisma/schema.prisma",
  "dependencies": {
    "@aws-sdk/lib-storage": "^3.645.0",
    "@prisma/client": "^5.22.0",
    "axios": "^1.7.9",
    "body-parser": "^1.20.2",
    "canvas": "^2.11.2",
    "discord-hybrid-sharding": "github:meister03/discord-hybrid-sharding#ts-rewrite",
    "discord.js": "^14.18.0",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "express-rate-limit": "^7.5.0",
    "winston": "^3.13.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.7",
    "@types/shelljs": "^0.8.15",
    "copyfiles": "^2.4.1",
